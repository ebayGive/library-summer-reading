<div class="hero-unit">
  <h2 class="name"><%= gridIcon.name %></h2>
  <%- linkTo('Edit this gridIcon', editGridIconPath(params.id), {class: 'btn pull-right'}); %>
  <%- linkTo('Back to list', gridIconPath(''), {class: 'btn pull-right'}); %>
</div>

<h2>GridIcon Properties</h2>
<% for(var i in gridIcon) { -%>
<% if(i !== 'saved') { -%>
<div class="row list-item">
  <div class="span4">
    <h3><%= i %></h3>
  </div>
  <div class="span8">
    <h3><i class="icon-list-alt"></i><span class="<%= i %>"><%= gridIcon[i] %></span></h3>
  </div>
</div>
<% } -%><% } -%>