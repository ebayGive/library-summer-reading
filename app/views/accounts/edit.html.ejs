<div class="row">
  <div class="col-md-6">
    <h3>Edit Account</h3>
  </div>
  <div class="col-md-6 text-right">
	<% if(session.get('account_info') && session.get('account_info').role && (session.get('account_info').role == "ADMIN" || session.get('account_info').role == "VOLUNTEER"))  { %>
      <a href="#" onclick="deleteAccount();return false;" class="btn btn-default">Delete</a>
      <a href="/accounts/<%=params['id']%>/edit_users" class="btn btn-default" title="Manage Reading Challenge">Reading Challenge</a>
	<% } %>
    <a href="#" onclick="saveAccount(saveAccountSuccessOnUpdate);return false;" class="btn btn-primary">Save</a>
  </div>
</div>
      <%- partial('/partial/message', {flash: flash}) %>

      <%- partial('form', {params: params, session: session, account: account, userTypes: userTypes, branches: branches, roles: roles}) %>
